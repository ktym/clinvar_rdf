  <Title>NM_000668.5(ADH1B):c.1108C&gt;T (p.Arg370Cys) AND Alcohol dependence</Title>
  <ReferenceClinVarAssertion DateCreated="2012-08-13" DateLastUpdated="2017-05-07" ID="77318">
    <ClinVarAccession Acc="RCV000019815" Version="2" Type="RCV" DateUpdated="2017-05-07"/>
